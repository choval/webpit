{
    "name": "choval/webpit",
    "description": "A self contained Rest API for converting image/videos to WebP",
    "type": "project",
    "authors": [
        {
            "name": "Osvaldo Jiang",
            "email": "osvaldo@publitar.com"
        }
    ],
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/clue/reactphp-sqlite"
      },
      {
        "type": "vcs",
        "url": "https://github.com/reactphp/filesystem"
      }
    ],
    "autoload": {
      "files": [
        "src/constants.php"
      ],
      "psr-4": {
        "Webpit\\": "src/"
      }
    },
    "require": {
        "clue/reactphp-sqlite": "dev-master",
        "react/http": "^0.8.4",
        "react/filesystem": "dev-master",
        "wyrihaximus/react-http-middleware-with-headers": "^1.0",
        "clue/block-react": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    }
}
